{
  "name": "@win-ui/components",
  "version": "2.0.7",
  "description": "Component Library for React applications, built with Tailwind CSS and TypeScript.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-tailwind": "tailwindcss -i ./src/styles/styles.css -o ./dist/styles.css --minify",
    "build-storybook": "storybook build",
    "build-components": "tsup src/index.ts --format esm,cjs --dts --external react --minify",
    "dev": "npm run build-tailwind && tsup src/index.ts --format esm,cjs --dts --external react --watch",
    "build": "npm run build-tailwind && npm run build-components",
    "release": "npm publish --access public"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "keywords": [],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "author": "Anderson Silva",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@semantic-release/npm": "^12.0.2",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-webpack5": "^9.1.5",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.12",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
    "semantic-release": "^24.2.7",
    "storybook": "^9.1.5",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
